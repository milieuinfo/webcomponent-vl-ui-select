{
  "name": "vl-ui-select",
  "version": "2.1.0",
  "description": "De \"select\" UI component van Webuniversum Vlaanderen als Web Component",
  "main": "vl-select.js",
  "scripts": {
    "prepare": "npm update --dev vl-ui-util && npm run prepare:util",
    "prepare:util": "npm explore vl-ui-util -- npm run util",
    "test": "wct -l chrome,firefox --npm",
    "test:grid": "echo TODO",
    "release": "np",
    "release:prepare": "npm run release:prepare:build && npm run release:prepare:imports && npm run release:prepare:css && npm run release:prepare:commit",
    "release:prepare:build": "minify *.src.js > vl-select.js",
    "release:prepare:imports": "replace '../dist/select.js' '/node_modules/vl-ui-select/dist/select.js' vl-select.js",
    "release:prepare:css": "replace '\\.\\./style.css' '/node_modules/vl-ui-select/style.css' vl-select.js",
    "release:prepare:commit": "git add -f vl-select.js && git commit --amend --no-edit && git pull",
    "release:testless": "np --yolo",
    "demo": "http-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/milieuinfo/webcomponent-vl-ui-select.git"
  },
  "author": "DIDM",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/milieuinfo/webcomponent-vl-ui-select/issues"
  },
  "homepage": "https://github.com/milieuinfo/webcomponent-vl-ui-select#readme",
  "dependencies": {
    "@govflanders/vl-ui-core": "^3.9.1",
    "@govflanders/vl-ui-select": "^3.9.1",
    "@govflanders/vl-ui-util": "^3.9.1",
    "vl-ui-core": "^3.1.1"
  },
  "peerDependencies": {
    "vl-ui-core": "^3.1.1"
  },
  "devDependencies": {
    "@govflanders/vl-ui-input-field": "^3.9.1",
    "vl-ui-button": "^1.3.1",
    "vl-ui-form-grid": "^1.0.12",
    "vl-ui-modal": "^1.0.13",
    "vl-ui-upload": "^1.0.2",
    "vl-ui-util": "^3.4.4"
  },
  "publishConfig": {
    "registry": "http://artifactory-pr-build.lb.cumuli.be:8081/artifactory/api/npm/acd-npm/"
  }
}
